#include <iostream>
#include <queue>
using namespace std;
int main(){
    string s="a a b c b";   
    queue<char> q;
    int freq[256]={0};    // frequency table for all chars
    for(char ch : s){
        if(ch==' ')continue; 
        freq[ch]++;      
        q.push(ch);      
        // remove repeating chars from front
        while(!q.empty() && freq[q.front()]> 1){
            q.pop();
        }
        if(q.empty())
            cout<<"-1 ";
        else
            cout<<q.front()<< " ";
    }
    return 0;
}
